{
  "name": "launchdarkly-js-sample",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "postinstall": "mkdir -p public && cp -r node_modules/weather-icons/css public/ && cp -r node_modules/weather-icons/font public/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e"
  },
  "dependencies": {
    "@launchdarkly/observability": "^0.4.8",
    "@launchdarkly/session-replay": "^0.4.8",
    "launchdarkly-js-client-sdk": "^3.9.0",
    "weather-icons": "^1.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/ui": "^4.0.14",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.2.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  }
}
